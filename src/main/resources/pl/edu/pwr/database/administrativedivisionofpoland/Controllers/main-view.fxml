                    <Tab id="0" text="Województwa">
                        <AnchorPane id="pane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="900.0">
                            <VBox id="vbox" prefHeight="600.0" prefWidth="1144.0">
                        <HBox prefHeight="500.0" prefWidth="856.0">
                           <children>
                               <TableView id="tableView" fx:id="voivodeshipsTableManage" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="698.0" />
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="voivodeshipTabAddUnitButton" mnemonicParsing="false" text="Dodaj" />
                                    <Button fx:id="voivodeshipTabEditUnitButton" mnemonicParsing="false" onAction="#onEditButtonClick" text="Edytuj" />
                                    <Button mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Usuń" />
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                                <Button mnemonicParsing="false" onAction="#onManageBackButtonClick" text="Cofnij" />
                                <Button mnemonicParsing="false" onAction="#onManageRefreshButtonClick" text="Odśwież" />
                            </VBox>
                        </AnchorPane>
                    </Tab>
                    <Tab id="1" text="Powiaty">
                        <AnchorPane id="pane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="900.0">
                            <VBox id="vbox" prefHeight="700.0" prefWidth="1098.0">
                        <HBox prefHeight="500.0" prefWidth="900.0">
                           <children>
                               <TableView id="tableView" fx:id="countiesTableManage" prefHeight="500.0" prefWidth="700.0" />
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="countyTabAddUnitButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="34.0" text="Dodaj" />
                                    <Button fx:id="countyTabEditUnitButton" mnemonicParsing="false" onAction="#onEditButtonClick" text="Edytuj" />
                                    <Button mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Usuń" />
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                                <Button mnemonicParsing="false" onAction="#onManageBackButtonClick" text="Cofnij" />
                                <Button mnemonicParsing="false" onAction="#onManageRefreshButtonClick" text="Odśwież" />
                            </VBox>
                        </AnchorPane>
                    </Tab>
                    <Tab id="2" text="Gminy">
                        <AnchorPane id="pane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="900.0">
                            <VBox id="vbox" prefHeight="700.0" prefWidth="1102.0">
                        <HBox prefHeight="500.0" prefWidth="900.0">
                           <children>
                                      <TableView id="tableView" fx:id="communesTableManage" prefHeight="500.0" prefWidth="667.0" />
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="communeTabAddUnitButton" mnemonicParsing="false" prefHeight="33.0" prefWidth="30.0" text="Dodaj" />
                                    <Button fx:id="communeTabEditUnitButton" mnemonicParsing="false" onAction="#onEditButtonClick" text="Edytuj" />
                                    <Button mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Usuń" />
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                                <Button mnemonicParsing="false" onAction="#onManageRefreshButtonClick" text="Odśwież" />
                            </VBox>
                        </AnchorPane>
                    </Tab>
                    <Tab id="3" text="Przeglądaj zgłoszenia">
                        <AnchorPane id="pane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="900.0">
                            <VBox id="vbox" prefHeight="700.0" prefWidth="900.0">
                                <TableView id="tableView" fx:id="reportsTableManage" />
                                <Button mnemonicParsing="false" onAction="#onManageRefreshButtonClick" text="Odśwież" />
                            </VBox>
                        </AnchorPane>
                    </Tab>
                </TabPane>
            </AnchorPane>
        </Tab>
        <Tab id="login" text="Logowanie">
            <AnchorPane id="pane" prefHeight="700.0" prefWidth="900.0">
                <VBox id="vbox" alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="10.0">
                    <TextField fx:id="loginTextField" alignment="CENTER" maxWidth="100.0" />
                    <PasswordField fx:id="passwordTextField" alignment="CENTER" maxWidth="100.0" />
                    <Button alignment="CENTER" onAction="#onLoginButtonClick" text="Login" />
                    <Label fx:id="loginFeedbackLabel" visible="false" />
                </VBox>
            </AnchorPane>
        </Tab>
    </TabPane>
</VBox>
