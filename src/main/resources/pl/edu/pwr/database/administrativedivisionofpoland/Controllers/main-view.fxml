<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.pwr.database.administrativedivisionofpoland.Controllers.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="22.0" prefWidth="900.0">
      <children>
         <Button fx:id="reportProblemButton" mnemonicParsing="false" onAction="#onReportProblemButtonClick" text="Zgłoś problem" />
      </children>
   </HBox>
    <TabPane id="pane" fx:id="mainTabPane" prefHeight="700.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE">
        <Tab id="view" text="Przeglądaj">
            <HBox>
                <AnchorPane id="pane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="900.0">
                    <TabPane fx:id="viewUnitsTabPane" prefHeight="700.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE">
                        <Tab id="0" text="Województwa">
                            <AnchorPane id="pane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="900.0">
                        <HBox prefHeight="104.0" prefWidth="900.0">
                           <children>
                                      <VBox prefHeight="307.0" prefWidth="464.0">
                                          <TableView id="tableView" fx:id="voivodeshipsTable" />
                                          <Button mnemonicParsing="false" onAction="#onBackButtonClick" text="Cofnij" />
                                          <Button mnemonicParsing="false" onAction="#onRefreshButtonClick" text="Odśwież" />
                                 <padding>
                                    <Insets left="10.0" top="10.0" />
                                 </padding>
                                      </VBox>
                              <VBox alignment="TOP_CENTER" prefHeight="104.0" prefWidth="436.0">
                                 <children>
                                    <Label visible="false" fx:id="viewingLabel" alignment="CENTER" prefHeight="17.0" prefWidth="192.0" text="Przeglądzasz" />
                                    <ChoiceBox visible="false" fx:id="communeOrCountyChoiceBox" prefHeight="25.0" prefWidth="69.0" />
